% layout 'default';
% title 'Browse';

%= $document->{filename}
%= $document->{size}

% if ($document->{tesseract_done}) {
<ul>
%     for my $ocr_text (map { $_->[0] } sort { $a->[1] cmp $b->[1] } map {[$_, sprintf("%09d%s",$_->{page_number},$_->{ocr_id}) ]} @{ $document->{tesseract_output} ||[]}) {
<li>
Page: <%= $ocr_text->{page_number} %>
<%= $ocr_text->{text} %>
(<%= $ocr_text->{bbox_csv} %>)
</li>
%     }
</ul>
% }
